{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "strict": true,
    "target": "ES2022"
    //    "noImplicitAny": false
  },
  "typeAcquisition": {
    "enable": true
  },
  "include": [
    "src"
  ]
}